            <class name="org.openlmis.functional.ManageRights">
            </class>
            <class name="org.openlmis.functional.ConfigureProgramTemplate">
            </class>
          <class name="org.openlmis.functional.RnRPagination">
          </class>
        </classes>
    </test>
</suite>

