<suite name="Openlmis Funtional Test" verbose="10" allow-return-values="true">
  <parameter name="browser" value="firefox"/>
  <parameter name="baseurl" value="http://localhost:9092/"/>
  <parameter name="dburl" value="jdbc:postgresql://localhost:5432/open_lmis_ft"/>
  <test name="Openlmis functional test">
    <groups>
      <run>
        <include name="functional"/>
      </run>

    </groups>
    <classes preserver-order="true">
      <class name="org.openlmis.functional.E2EManageFacility">
      </class>
      <class name="org.openlmis.functional.E2EUpload">
      </class>
      <class name="org.openlmis.functional.ViewRequisition">
      </class>
        <class name="org.openlmis.functional.ManageRights">
        </class>
      <class name="org.openlmis.functional.ConfigureProgramTemplate">
      </class>
    </classes>
  </test>
</suite>

