<suite name="Openlmis Distribution Test" verbose="10" allow-return-values="true">
  <test name="Openlmis distribution test">
    <groups>
      <run>
        <include name="distribution"/>
      </run>
    </groups>
    <classes preserver-order="true">
      <class name="org.openlmis.functional.ManageDistribution">
      </class>
      <class name="org.openlmis.functional.ViewWarehouseLoadAmount">
      </class>
      <class name="org.openlmis.functional.ManageRefrigerator">
      </class>
      <class name="org.openlmis.functional.DistributionSyncTest">
      </class>
      <class name="org.openlmis.functional.DistributionEpiUseSyncTest">
      </class>
      <class name="org.openlmis.functional.DistributionRefrigeratorSyncTest">
      </class>
      <class name="org.openlmis.functional.DistributionEpiInventoryTest">
      </class>
    </classes>
  </test>
</suite>

