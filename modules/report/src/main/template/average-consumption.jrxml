	<queryString>
		<![CDATA[select avg(quantitydispensed) average, product, productcategory category, ft.name facilityType, f.name facilityName, MAX(s.name) supplyingFacility, MAX(li.maxmonthsofstock) MaxMOS, MAX(li.maxmonthsofstock) minMOS
        from requisition_line_items li
        inner join requisitions r on r.id = li.rnrid
        inner join facilities f on r.facilityid = f.id
        inner join facility_types ft on ft.id = f.typeid
        inner join processing_periods pp on pp.id = r.periodid
        inner join products pr on pr.code = li.productcode
        inner join product_categories prc on prc.id = pr.categoryid
        inner join requisition_group_members rgm on rgm.facilityid = f.id
        inner join supply_lines sl on sl.supervisorynodeid = r.supervisorynodeid and r.programid = sl.programid
inner join facilities s on s.id = sl.supplyingfacilityid
 group by li.product, li.productcategory,  f.name, ft.name
 order by li.productCategory, li.product]]>
